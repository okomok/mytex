\ProvidesPackage{my}

\usepackage{amsmath,mathtools,etoolbox,lambda,graphicx}

\newcommand*{\myflip}[3]{#1{#3}{#2}}

\newcommand*{\mydefeq}{\coloneqq}
\newcommand*{\myeqdef}{\eqqcolon}

\newcommand*{\mycomment}[1]{}

\newcommand*\myassign[2]{%
    \global\csdef{#1}{#2}
    #2%
}

\newcommand*{\mydash}{\text{--}}

\newcommand*{\myforall}[2]{%
    \ensuremath{({} ^ \forall #1)(#2)}%
}

\newcommand*{\myexists}[2]{%
    \ensuremath{({} ^ \exists #1)(#2)}%
}

\def\myin#1#2{\Foldr{\myinimpl}{#2}{\myListize{#1}}}
\newcommand*{\myinimpl}[2]{\begin{#1}#2\end{#1}}

\newcommand*{\myprime}{^{\prime}\mkern-1.2mu}
\newcommand*{\mydprime}{^{\prime\prime}\mkern-1.2mu}
\newcommand*{\mytrprime}{^{\prime\prime\prime}\mkern-1.2mu}

\newcommand*{\myitem}{\>\raisebox{0.13ex}{\scalebox{0.85}{\bullet}}\ }
\newcommand*{\mynitem}[1]{\>\text{#1}.\ }

\newcommand*{\mydint}[1]{\ensuremath{\displaystyle\int_{\mathrlap{#1}}\;}}

\def\myUnlistize#1{#1\myUnlistize@{}}
\def\myUnlistize@#1{#1\Foldr\Commaize{}}

\def\myListize#1%
   {\myListize@#1,\relax\@eolst}
\def\myListize@#1,#2\@eolst%
   {\TeXif{\ifx\relax#2}%
        {\Singleton{#1}}%
        {\Cons{#1}{\myListize@#2\@eolst}}}

