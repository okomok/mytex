\ProvidesPackage{my}

\usepackage{amsmath,mathtools,etoolbox}

\newcommand{\mydefeq}{\coloneqq}

\newcommand{\myidentity}[1]{#1}
\newcommand{\mycomment}[1]{}

\newcommand\myassign[2]{%
    \global\csdef{#1}{#2}
    #2%
}

\newcommand{\myfamily}[2]{%
    \left(#2\right)_{\!\!\!#1}%
}

\newcommand{\mydash}{\text{--}}

\newcommand{\myforall}[2]{%
    \ensuremath{({} ^ \forall #1)(#2)}%
}

\newcommand{\myexists}[2]{%
    \ensuremath{({} ^ \exists #1)(#2)}%
}

\newcommand{\mymath}[1]{%
    \begin{math}
        #1
    \end{math}%
}

\newcommand{\myequation}[1]{%
    \begin{equation}
        #1
    \end{equation}%
}

