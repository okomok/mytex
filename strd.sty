\tikzset{strd cell/.style={strd cellbase,very thin}}
\tikzset{strd mapcell/.style={strd cellbase,thin}}
\tikzset{strd bijcell/.style={strd cellbase,very thin,sharp corners}}

\newcommand*{\strdputcellof}[3]{% {style}{text}{node}
    \strdonlayer@{strd celllayer}{
        \node[#1] at (#3) {#2};
    }% 
}
\newcommand*{\strdputcell}{\strdputcellof{strd cell}}
\newcommand*{\strdputmapcell}{\strdputcellof{strd mapcell}}
\newcommand*{\strdputbijcell}{\strdputcellof{strd bijcell}}


% Frames
%

\tikzset{strd framebase/.style={solid,semithick}}
\tikzset{strd frame/.style={strd framebase,rounded corners=2pt}}

\newcommand*{\strdframe}{\Compose\strdseqin\strdframeone}
\newcommand*{\strdframeone}{\Compose{\strdonlayer{strd framelayer}}{\strdscope{strd frame}}}


% Boxes
%

\tikzset{strd box/.style={strd framebase,rounded corners=2pt}}
\tikzset{strd bijbox/.style={strd framebase,sharp corners}}

\newcommand*{\strdboxof}[3]{\strdseqin{\strdboxofone{#1}{#2}{#3}}}
\newcommand*{\strdboxofone}[7]{% {style}{xl}{xr}{stm}{sc}{newid}{previd}
    \strduse{#5}{#6 Prev}{#7}
    \strduse{#4}{\strdemptyC}{#6 Unframe}{#6 Prev}
    \strdonlayer@{strd framelayer}{
        \strdpathis@{#1}{
            \strdxscale@{#2}{ \strdsqsubsettoC{#6 Left}{#6 Prev}{#6 Unframe} }
            \strdxscale@{#3}{ \strdsqsupsettoC{#6 Right}{#6 Prev}{#6 Unframe} }
            \strdjumpto@{#6}{#6 Prev End}{#6 Unframe End}
         }
     }%
}
\newcommand*{\strdbox}{\strdboxof{strd box}}
\newcommand*{\strdbijbox}{\strdboxof{strd bijbox}}

\newcommand*{\strddeclsqsubset@}[3]{% {newid}{fromnode}{tonode}
    \strdcoord@{#1 Start}{#2}
    \strdcoord@{#1 StartCorner}{$(#2)+(-\strdxunit,0)$}
    \strdcoord@{#1 EndCorner}{$(#3)+(-\strdxunit,0)$}
    \strdcoord@{#1 Mid}{$(#1 StartCorner)!.5!(#1 EndCorner)$}
    \strdcoord@{#1}{#1 Mid}
    \strdcoord@{#1 End}{#3}
}
\newcommand*{\strddeclsqsupset}[3]{\strdflip@{\strddeclsqsubset@{#1}{#2}{#3}}}


% Tubes
%

\tikzset{strd tube/.style={strd framebase}}

% Constants
\newcommand*{\strdtubexunit}{0.75}
\newcommand*{\strdtubeyunit}{0.8}
\newcommand*{\strdtubexhalfunit}{0.375}
\newcommand*{\strdtubeyhalfunit}{0.4}
\newcommand*{\strdtubexmagic}{0.125}

% start
\newcommand*{\strdtubestart}{\strdcomposeC\strdtubestartC}
\newcommand*{\strdtubestartC}[2]{% {newid}{previd}
    \strdempty@{#1 Left}{$(#2 End)+(-\strdtubexhalfunit,0)$}
    \strdempty@{#1}{#2 End}
    \strdempty@{#1 Right}{$(#2 End)+(\strdtubexhalfunit,0)$}
}

% helpers
\newcommand*{\strdtube@}[1]{
    \strdonlayer@{strd framelayer}{
        \strdpathis@{strd tube}{
            #1 
        }
    }%
}

% mid
\newcommand*{\strdtubemid}{\strdcomposeC\strdtubemidC}
\newcommand*{\strdtubemidC}[2]{% {newid}{previd}
    \strdtube@{
        \strdmidC{#1 Left}{#2 Left}
        \strdjumpC{#1}{#2}
        \strdmidC{#1 Right}{#2 Right}
    }%
}

% left
\newcommand*{\strdtubeleft}{\strdcomposeC\strdtubeleftC}
\newcommand*{\strdtubeleftC}[2]{% {newid}{previd}
    \strdtube@{
        \strdmidC{#1 Left}{#2 Left}
        \strdljumpC{#1 Impl}{#2}
        \strdjumpC{#1}{#1 Impl}
        \strdtubeleftrightC{#1 Right}{#2}
    }%
}
\newcommand*{\strdtubeleftrightC}[2]{% {newid}{previd}
    \strdcoord@{#1 Start}{$(#2 End)+(0,-\strdtubeyhalfunit)$}
    \strdcoord@{#1 StartCorner}{$(#1 Start)+(-\strdtubexmagic,0)$}
    \strdcoord@{#1 End}{$(#1 StartCorner)+(0,-\strdtubeyhalfunit)$}
    \strdcoord@{#1 Mid}{$(#1 StartCorner)!.5!(#1 End)$}
    \strdcoord@{#1}{#1 Mid}
    \strddraw@{ (#1 Start) -- (#1 StartCorner) -- (#1 End) }%
}

% right
\newcommand*{\strdtuberight}{\strdcomposeC\strdtuberightC}
\newcommand*{\strdtuberightC}[2]{% {newid}{previd}
    \Compose\strdtube@\strdflip@{
        \strdmidC{#1 Right}{#2 Right}
        \strdljumpC{#1 Impl}{#2}
        \strdjumpC{#1}{#1 Impl}
        \strdtubeleftrightC{#1 Left}{#2}
    }%
}

% unleft
\newcommand*{\strdtubeunleft}{\strdcomposeC\strdtubeunleftC}
\newcommand*{\strdtubeunleftC}[2]{% {newid}{previd}
    \strdtube@{
        \strdmidC{#1 Left}{#2 Left}
        \strdrjumpC{#1 Impl}{#2}
        \strdjumpC{#1}{#1 Impl}
        \strdtubeunleftrightC{#1 Right}{#2 Right}
    }%
}
\newcommand*{\strdtubeunleftrightC}[2]{% {newid}{previd}
    \strdcoord@{#1 Start}{#2 End}
    \strdcoord@{#1 StartCorner}{$(#1 Start)+(0,-\strdtubeyhalfunit)$}
    \strdcoord@{#1 EndCorner}{$(#1 StartCorner)+(\strdtubexmagic,0)$}
    \strdcoord@{#1 Mid}{$(#1 StartCorner)!.5!(#1 EndCorner)$}
    \strdcoord@{#1}{#1 Mid}
    \strdcoord@{#1 End}{$(#1 EndCorner)+(0,-\strdtubeyhalfunit)$}%+(0.7625,0)$}
    \strddraw@{ (#1 Start) -- (#1 StartCorner) -- (#1 EndCorner) }
}

% unright
\newcommand*{\strdtubeunright}{\strdcomposeC\strdtubeunrightC}
\newcommand*{\strdtubeunrightC}[2]{% {newid}{previd}
    \Compose\strdtube@\strdflip@{
        \strdmidC{#1 Right}{#2 Right}
        \strdrjumpC{#1 Impl}{#2}
        \strdjumpC{#1}{#1 Impl}
        \strdtubeunleftrightC{#1 Left}{#2 Left}
    }%
}

% merge
\newcommand*{\strdtubemerge}[5]{% {lstm}{rstm}{sc}{newid}{previd}
    \strduse{#3}{#4 Prev}{#5}
    \strduse{#1}{\strdemptyC}{#4 LeftBranch}{#4 Prev}
    \strduse{#2}{\strdemptyC}{#4 RightBranch}{#4 Prev}
    \strdempty@{#4 Left}{#4 LeftBranch Left End}
    \strdempty@{#4 Right}{#4 RightBranch Right End}
    \strdempty@{#4}{$(#4 Left)!.5!(#4 Right)$}%
}

\newcommand*{\DimEq}[2]{\Iftex{\ifdim#1=#2 }}

% extend{lscale}{rscale} = yscale{0}{dilate{lscale}{rscale}}
\newcommand*{\strdtubeextend}[2]{\strdcomposeC{\strdtubeextendC{#1}{#2}}}
\newcommand*{\strdtubeextendC}[4]{% {lscale}{rscale}{newid}{previd}
    \strdtube@{
        \If{\DimEq{#1pt}{0pt}}{
            \strdempty@{#3 Left}{#4 Left End}
        }{
            \strdxscale@{#1}{\strdempty@{#3 Left}{$(#4 Left End)+(-\strdxunit,0)$}}
        }
        \strdempty@{#3}{#4 End}
        \If{\DimEq{#2pt}{0pt}}{
            \strdempty@{#3 Right}{#4 Right End}
        }{
            \strdxscale@{#2}{\strdempty@{#3 Right}{$(#4 Right End)+(\strdxunit,0)$}}
        }
    }%
}

% unextend{lscale}{rscale} = yscale{0}{undilate{lscale}{rscale}}
\newcommand*{\strdtubeunextend}[2]{\strdcomposeC{\strdtubeunextendC{#1}{#2}}}
\newcommand*{\strdtubeunextendC}[4]{% {lscale}{rscale}{newid}{previd}
    \strdflip@{
        \strdtubeextendC{#1}{#2}{#3}{#4}
    }%
}

% dilate{lscale}{rscale}
\newcommand*{\strdtubedilate}[2]{\strdcomposeC{\strdtubedilateC{#1}{#2}}}
\newcommand*{\strdtubedilateC}[4]{% {lscale}{rscale}{newid}{previd}
    \strdtube@{
        \If{\DimEq{#1pt}{0pt}}{
            \strdmidC{#3 Left}{#4 Left}
        }{
            \strdxscale@{#1}{\strdtubedilateleftC{#3 Left}{#4 Left}}
        }
        \strdjumpC{#3}{#4}
        \If{\DimEq{#2pt}{0pt}}{
            \strdmidC{#3 Right}{#4 Right}
        }{
            \strdxscale@{#2}{\strdflip@{\strdtubedilateleftC{#3 Right}{#4 Right}}}
        }
    }%
}
\newcommand*{\strdtubedilateleftC}[2]{% {newid}{previd}
    \strdcoord@{#1 Start}{#2 End}
    \strdcoord@{#1 StartCorner}{$(#1 Start)+(0,-\strdtubeyhalfunit)$}
    \strdcoord@{#1 EndCorner}{$(#1 StartCorner)+(-\strdxunit,0)$}
    \strdcoord@{#1 Mid}{$(#1 StartCorner)!.5!(#1 EndCorner)$}
